{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@pages/*": ["./src/pages/*"],
      "@theme/*": ["./src/globalStyles/*"],
      "@components/*": ["./src/components/*"],
      "@assets/*": ["./assets/*"],
      "@utils/*": ["./src/utils/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@gcomponents/*": ["./src/globalStyles/globalComponents/*"]
    }
  }
}
